{
  "name": "n_pdf",
  "version": "0.0.3",
  "description": "Node.js native module bindings for PoDoFo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git+https://github.com/corymickelson/npdf.git",
  "scripts": {
    "compile": "cmake-js compile --std c++17 --prefer-clang",
    "postinstall": "npm run compile && npm run lib-build",
    "lib-build": "tsc --p tsconfig.json",
    "docs": "typedoc --mode modules --module commonjs --target es6 --out ./docs",
    "test": "npm run test:document && npm run test:page && npm run test:field && npm run test:object",
    "test:document": "tape ./dist/document.spec.js | tap-spec",
    "test:page": "tape ./dist/page.spec.js | tap-spec",
    "test:field": "tape ./dist/field.spec.js | tap-spec",
    "test:object": "tape ./dist/object.spec.js | tap-spec",
    "build:test": "npm run compile && npm run lib-build && npm run test",
    "precommit": "npm run compile && npm run lib-build && npm test",
    "preinstall": "node ./checkDeps.js"
  },
  "keywords": [
    "pdf"
  ],
  "author": "Cory Mickelson <mickelsoncory@gmail.com> (http://corymickelson.github.io/)",
  "homepage": "https://github.com/corymickelson/npdf#readme",
  "bugs": {
    "url": "https://github.com/corymickelson/npdf/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^8.0.28",
    "@types/tape": "^4.2.30",
    "husky": "^0.14.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "typedoc": "^0.8.0",
    "typescript": "~2.5.3"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "cmake-js": "^3.5.0",
    "node-addon-api": "1.0.0"
  }
}
